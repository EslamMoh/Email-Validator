<%if @response.key? :error%>
	<p>
	  <strong>Please insert an email ! </strong>
	</p>
<%else%>
<table class="table table-hover">
  <thead>
    <tr>
    	<th scope="col">#</th>
      	<th scope="col">Email</th>
		<th scope="col">Did You Mean</th>
		<th scope="col">User</th>
		<th scope="col">Domain</th>
		<th scope="col">Format Valid</th>
		<th scope="col">Mx Found</th>
		<th scope="col">Smtp Check</th>
		<th scope="col">Catch All</th>
		<th scope="col">Role</th>
		<th scope="col">Disposable</th>
		<th scope="col">Free</th>
		<th scope="col">Score</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
	<td><%= @response[:email] %></td>
	<td><%= @response[:did_you_mean] %></td>
	<td><%= @response[:user] %></td>
	<td><%= @response[:domain] %></td>
	<td><%= @response[:format_valid] %></td>
	<td><%= @response[:mx_found] %></td>
	<td><%= @response[:smtp_check] %></td>
	<td><%= @response[:catch_all] %></td>
	<td><%= @response[:role] %></td>
	<td><%= @response[:disposable] %></td>
	<td><%= @response[:free] %></td>
	<td><%= @response[:score] %></td>
    </tr>
  </tbody>
</table>
<%end%>
<%= link_to 'Back', new_email_path%>